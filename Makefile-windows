define newline


endef

C_SOURCES := $(notdir $(wildcard src/*.c))

all:
	$(foreach C_SOURCE, $(C_SOURCES), gcc src/$(C_SOURCE) -lm -o bin/$(C_SOURCE).o -c -Ofast$(newline))
	gcc -lm -o superforth $(wildcard bin/*.c.o) -Ofast

fook:
	$(foreach C_SOURCE, $(C_SOURCES), gcc src/$(C_SOURCE) -lm -o bin/$(C_SOURCE).o -c -g -ggdb$(newline))
	gcc -lm -o superforth $(wildcard bin/*.c.o) -g -ggdb
